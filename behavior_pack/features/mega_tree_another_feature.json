
{
	"format_version": "1.15.0",
	"minecraft:tree_feature": {
		"description": {
			"identifier": "gaiawanderer:mega_tree_another_feature"
		},
		"may_grow_on": [
			"minecraft:grass",
			"minecraft:dirt",
			"minecraft:podzol",
			"minecraft:sand",
			{
				"name": "minecraft:dirt",
				"states": {
					"dirt_type": "coarse"
				}
			}
		],
		"may_replace": [
			"minecraft:air",
			"minecraft:leaves",
			"minecraft:dirt",
			"minecraft:seaweed",
			"minecraft:tallgrass",
			"minecraft:grass",
			"minecraft:water",
			"minecraft:flowing_water",
			"minecraft:waterlily",
			"minecraft:log",
			"minecraft:log2",
			"minecraft:vine",
			"minecraft:red_mushroom_block",
			"minecraft:brown_mushroom_block",
			"minecraft:red_mushroom",
			"minecraft:brown_mushroom",
			"minecraft:moss_block",
			"gaiawanderer:mana_leaves"
		],
		"acacia_trunk": {
			"trunk_block": {
				"name": "minecraft:log"
			},
			"trunk_width": 3,
			"trunk_height": {
				"base": 23,
				"intervals": [1, 7]
			},
			"trunk_lean": {
				"allow_diagonal_growth": true,
				"lean_height": {
					"range_min": 8,
					"range_max": 16
				},
				"lean_steps":{
					"range_min": 2,
					"range_max": 4
				}
			},
			"branches": {
				"branch_position":{
					"range_min": 2,
					"range_max": 6
				},
				"branch_chance": {
					"numerator": 3,
					"denominator": 4
				},
				"branch_length": {
					"range_min": 3,
					"range_max": 6
				},
				"branch_canopy": {
					"canopy": {
						"leaf_block": {
							"name": "minecraft:leaves"
						},
						"canopy_offset": {
							"min": -1,
							"max": 1
						},
						"min_width": 1,
						"canopy_slope": {
							"run": 4,
							"rise": 3
						},
						"variation_chance": {
							"numerator": 1,
							"denominator": 2
						},
						"canopy_decoration": {
							"decoration_block": {
								"name": "minecraft:vine"
							},
							"decoration_chance": {
								"numerator": 1,
								"denominator": 3
							},
							"num_steps": 7
						}
					}
				}
			}
		},
		"mega_canopy": {
			"leaf_block": {
				"name": "gaiawanderer:mana_leaves"
			},
			"canopy_height": 4,
			 "base_radius": 8,
			 "core_width": 3
		}
	}
}