{
	"format_version": "1.13.0",

	"minecraft:feature_rules": {
		"description": {
			"identifier": "gaiawanderer:ender_brick_feature_rule",
			"places_feature": "gaiawanderer:ender_brick_feature"
		},

		"conditions": {
			"placement_pass": "before_surface_pass",

			"minecraft:biome_filter": {
				"all_of": [
					{
						"any_of": [
							{
								"test": "has_biome_tag",
								"operator": "==",
								"value": "the_end"
							}
						]
					}
				]
			}
		},

		"distribution": {
			"iterations": 256,

			"x": {
				"distribution": "fixed_grid",
				"extent": [0, 15]
			},
			"z": {
				"distribution": "fixed_grid",
				"extent": [0, 15]
			},
			"y": "(q.noise(v.worldx / 1024, v.worldz / 1024) + q.noise(v.worldx, v.worldz) * 0.05 < -0.3 && (math.abs(v.worldx) > 200 || math.abs(v.worldz) > 200)) ? (q.heightmap(v.worldx, v.worldz) - 1) : -1"
		}
	}
}