            "event": "to_avoid"
          }
        },
        "minecraft:behavior.melee_attack": {
          "priority": 4
        },
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 3,
          "entity_types": [
            {
              "filters": {
                "any_of" : [
                  {"test" : "is_family", "subject" : "other", "operator": "!=", "value" :  "monster"},
                  {"test" : "is_family", "subject" : "other", "value" :  "necrocancer"}
                ]
              },
              "max_dist": 70
            }
          ],
          "must_see": false
        },
