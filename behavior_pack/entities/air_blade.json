{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "gaiawanderer:air_blade",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "remove": {
        "minecraft:despawn":{}
      },
      "start_riding": {
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "event": "ride"
            }
          ]
        }
      }
    },
    "components": {
      "minecraft:rideable": {
        "seat_count": 1,
        "interact_text": "action.interact.ride.boat",
        "family_types": [
          "player"
        ],
        "seats": [
          {
            "position": [ 0.0, 0.25, 0.0 ]
          }
        ]
      },
      "minecraft:projectile": {
        "on_hit": {
          "impact_damage": {
            "damage": 6,
            "knockback": true,
            "semi_random_diff_damage": true,
            "destroy_on_hit": false,
            "max_critical_damage": 10,
            "min_critical_damage": 9,
            "power_multiplier": 0.97
          },
          "stick_in_ground": {
            "shake_time" : 0.0
          },
          "arrow_effect": {
          },
          "remove_on_hit": {
          }
        },
        "hit_sound": "bow.hit",
        "power": 2.0,
        "gravity": 0.05,
        "uncertainty_base": 1,
        "uncertainty_multiplier": 0,
        "anchor": 1,
        "should_bounce": true,
        "offset": [ 0, -0.1, 0 ]
      },

      "minecraft:collision_box": {
        "width": 0.8,
        "height": 0.8
      },
      "minecraft:loot": {
        "table": ""
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:damage_sensor": {
        "on_damage": {
          "event": "remove_air_blade"
        }
      },
      "minecraft:conditional_bandwidth_optimization": {
        "default_values": {
          "max_optimized_distance": 10.0,
          "max_dropped_ticks": 2,
          "use_motion_prediction_hints": true
        }
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "add": {
              "component_groups": [
                "start_riding"
              ]
            }
          }
        ]
      },
      "ride": {
        "sequence": [
          {
            "run_command": {
              "command": "ride @p start_riding @s"
            }
          },
          {
            "remove": {
              "component_groups": [
                "start_riding"
              ]
            }
          }
        ]
      },
      "remove_air_blade": {
        "add": {
          "component_groups": [
            "remove"
          ]
        }
      }
    }
  }
}
