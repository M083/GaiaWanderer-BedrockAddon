{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "gaiawanderer:ender_proto",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },

    "component_groups": {
      "gaiawanderer:apoptosis": {
          "minecraft:instant_despawn": {}
      }
    },
    "components": {
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? 14 : 0"
      },
      "minecraft:type_family": {
        "family": [ "ender_proto", "undead", "monster", "ender" ]
      },
      "minecraft:breathable":{
        "total_supply":15,
        "suffocate_time":0,
        "breathes_air": true,
        "breathes_water": true
      },
      "minecraft:loot": {
        "table": "loot_tables/entities/ender_proto.json"
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1
      },
      "minecraft:attack": {
        "damage": 11
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "on_damage": {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "operator": "==",
                    "value": "ender"
                  }
                ]
              }
            },
            "deals_damage": false
          }
        ]
      },
      "minecraft:behavior.melee_attack": {
        "priority": 3,
        "track_target": false,
        "reach_multiplier": 1.5,
        "speed_multiplier": 1
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "entity_types": [
          {
            "filters": {
              "any_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "!=",
                  "value": "monster"
                },
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "founder"
                }
              ],
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "!=",
                  "value": "ender"
                }
              ]
            },
            "max_dist": 32
          }
        ],
        "must_see": true
      },
      "minecraft:collision_box": {
        "width": 1.75,
        "height": 3.25
      },

      "minecraft:movement": {
        "value": 0.025
      },
      "minecraft:flying_speed": {
        "value": 0.35
      },
      "minecraft:underwater_movement": {
        "value": 0.25
      },
      "minecraft:navigation.hover": {
        "can_path_over_water": true,
        "can_sink": false,
        "can_path_from_air": true,
        "avoid_water": true,
        "avoid_damage_blocks": true,
        "avoid_sun": false
      },
      "minecraft:behavior.random_hover": {
        "priority": 11,
        "xz_dist": 8,
        "y_dist": 12,
        "y_offset": -1,
        "interval": 1,
        "hover_height": [6, 12]
      },
      "minecraft:movement.hover": {},
      "minecraft:jump.static": {
      },
      "minecraft:behavior.look_at_player": {
        "priority": 7,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:can_fly": {
      },
      "minecraft:health": {
        "value": 80,
        "max": 80
      },
      "minecraft:teleport": {
        "random_teleports": true,
        "max_random_teleport_time": 5,
        "random_teleport_cube": [ 32, 32, 32 ],
        "target_distance": 16,
        "target_teleport_chance": 0.15,
        "light_teleport_chance": 0.15
      },
      "minecraft:nameable": {
      },
      "minecraft:physics": {
      },
      "minecraft:on_death": {
        "event": "death_event"
      }
    },
    "events": {
      "death_event": {
        "randomize": [
          {
            "weight": 99
          },
          {
            "weight": 1,
            "run_command": {
              "command": "structure load mystructure:book_ender_proto ~ ~ ~"
            }
          }
        ]
      },
      "gaiawanderer:apoptosis_event": {
        "add": {
          "component_groups": [
            "gaiawanderer:apoptosis"
          ]
        }
      }
    }
  }
}
