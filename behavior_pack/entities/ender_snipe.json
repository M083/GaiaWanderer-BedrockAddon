{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "gaiawanderer:ender_snipe",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },

    "component_groups": {
      "gaiawanderer:apoptosis": {
          "minecraft:instant_despawn": {}
      }
    },
    "components": {
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? 14 : 0"
      },
      "minecraft:type_family": {
        "family": [ "ender_snipe", "undead", "monster", "ender" ]
      },
      "minecraft:breathable":{
        "total_supply":15,
        "suffocate_time":0,
        "breathes_air": true,
        "breathes_water": true
      },
      "minecraft:loot": {
        "table": "loot_tables/entities/ender_snipe.json"
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "on_damage": {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "operator": "==",
                    "value": "ender"
                  }
                ]
              }
            },
            "deals_damage": false
          }
        ]
      },
      "minecraft:shooter": {
        "def": "minecraft:arrow"
      },
      "minecraft:behavior.ranged_attack": {
        "attack_radius": 64,
        "charge_shoot_trigger": 1.25,
        "charge_charged_trigger": 0.6,
        "burst_shots": 1,
        "burst_interval": 0
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "entity_types": [
          {
            "filters": {
              "any_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "!=",
                  "value": "monster"
                },
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "founder"
                }
              ],
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "!=",
                  "value": "ender"
                }
              ]
            },
            "max_dist": 64
          }
        ],
        "must_see": true
      },
      "minecraft:collision_box": {
        "width": 2.25,
        "height": 2.5
      },
      "minecraft:movement": {
        "value": 0.03
      },
      "minecraft:navigation.float": {
        "can_path_over_water": true
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:behavior.float": {
        "priority": 0
      },

      "minecraft:behavior.float_wander": {
        "priority": 10,
        "xz_dist": 3,
        "y_dist": 7,
        "y_offset": -2.0,
        "random_reselect": true,
        "float_duration": [ 0.1, 0.35 ]
      },
      "minecraft:can_fly": {
      },
      "minecraft:behavior.look_at_player": {
        "priority": 7,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:can_fly": {
      },
      "minecraft:health": {
        "value": 40,
        "max": 40
      },
      "minecraft:teleport": {
        "random_teleports": true,
        "max_random_teleport_time": 5,
        "random_teleport_cube": [ 32, 32, 32 ],
        "target_distance": 16,
        "target_teleport_chance": 0.15,
        "light_teleport_chance": 0.15
      },
      "minecraft:nameable": {
      },
      "minecraft:physics": {
      },
      "minecraft:on_death": {
        "event": "death_event"
      }
    },
    "events": {
      "death_event": {
        "randomize": [
          {
            "weight": 99
          },
          {
            "weight": 1,
            "run_command": {
              "command": "structure load mystructure:book_ender_snipe ~ ~ ~"
            }
          }
        ]
      },
      "gaiawanderer:apoptosis_event": {
        "add": {
          "component_groups": [
            "gaiawanderer:apoptosis"
          ]
        }
      }
    }
  }
}
